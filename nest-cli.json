{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.build.json",
    "typeCheck": true,
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "extensions": [".ts", ".json"],
        "outDir": "dist"
      }
    }
  }
}
